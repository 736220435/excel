<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springboot.dao.ComputerDao">
    <select id="computerList" resultType="computer">
        select id,name,price,cpu,disk,ram,"system" from computer
    </select>
    <insert id="insertComputer">
        insert into computer values
        <foreach collection="list" item="item" open="(" close=")" separator=",">
            #{item.id},#{item.name},#{item.price},#{item.cpu},#{item.disk},#{item.ram},#{item.system}
        </foreach>
    </insert>
</mapper>